<h2>Cargar Notas</h2>
    <form method="POST" action="CrearNota">
        <div class="form-group">
            <label for="titulo" >Titulo</label>
            <input class="form-control" name="titulo" placeholder="Ingrese su titulo" required/>
        </div>
        <div class="form-group">
            <label for="texto">Texto</label>
            <textarea class="form-control" name="texto" placeholder="Ingrese su texto" required></textarea>
        </div>
       
        <input type="submit" class="btn btn-primary" value="Enviar"/>
    </form>