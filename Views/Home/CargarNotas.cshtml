<h2>Cargar Notas</h2>
    <form method="POST" action="Index">
        <div class="form-group">
            <label for="mail" >Mail</label>
            <input class="form-control" name="mail" placeholder="Ingrese su mail" required/>
        </div>
        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input class="form-control" name="nombre" placeholder="Ingrese su nombre" required/>
        </div>
        <div class="form-group">
            <label for="nota">Ingrese su nota</label>
            <input class="form-control" name="nota"></input>
        </div>
        <input type="submit" class="btn btn-primary" value="Enviar"/>
    </form>